<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>ClientServlet</servlet-name>
		<servlet-class>com.campusnumerique.vehiclerental.servlet.client.ClientServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClientServlet</servlet-name>
		<url-pattern>/clients</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>CarServlet</servlet-name>
		<servlet-class>com.campusnumerique.vehiclerental.servlet.car.CarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CarServlet</servlet-name>
		<url-pattern>/cars</url-pattern>
	</servlet-mapping>

</web-app>